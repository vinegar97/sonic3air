function void LevelObjectTableBuilder.addObject(u16 px, u16 py, u8 type, u8 subtype, u8 flags)
{
	u16[A0]   = px
	u16[A0+2] = py | (flags << 13)
	u8[A0+4]  = type
	u8[A0+5]  = subtype
	A0 += 6
}
